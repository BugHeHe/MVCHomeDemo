<script>
    var a = new Vue({
        el: '.container',
        data: {
            QuestionS: [],
            Name: '',
            BookList: [{ BookName: "全部" }],
            GradeList: [],
            Chapter: [{ ChapterName: "全部" }]
        },
        created() {
            this.Init();
            this.In1();
        },
        methods: {
            Init() {
                axios.post('/ShowQuextion/List', { page: '0' }).then(Response => {
                    this.QuestionS = Response.data.Li;
                    $('#CuntYe').text(Response.data.Ye);
                    $('#Ye').text("1");
                })
            },
            In1() {
                axios.post('/ShowQuextion/GradeList').then(Response => {
                    this.GradeList = Response.data;
                })
            },
            GradeBian() {
                if ($('#GradeName').val() == "全部" || $('#GradeName').val() == "全部") {
                    this.Chapter = [{ ChapterName: "全部" }];
                    this.BookList = [{ BookName: "全部" }];
                } else {
                    axios.post('/ShowQuextion/BookList', { GradeID: $('#GradeName').val() }).then(Response => {
                        this.BookList = Response.data;
                    })
                }

            },
            BookBian() {
                if ($('#BookName').val() == "全部" || $('#GradeName').val() == "全部") {
                    this.Chapter = [{ ChapterName: "全部" }];
                    this.BookList = [{ BookName: "全部" }];
                } else {
                    axios.post('/ShowQuextion/ChapterList', { Chapter: $('#BookName').val() }).then(Response => {
                        this.Chapter = Response.data;
                    })
                }
            },
            Select() {
                axios.post('/ShowQuextion/Select', { page: 0, Title: $('#exampleInputEmail3').val(), GradeName: $('#GradeName').val(), BookName: $('#BookName').val(), ChapterName: $('#ChapterName').val() }).then(Response => {
                    this.QuestionS = Response.data.Li;
                    $('#CuntYe').text(Response.data.Ye);
                    if ($('#CuntYe').text() == "0")
                        $('#Ye').text("0");
                    else
                        $('#Ye').text("1");
                })
            },
            //首页
            ShouYe() {
                var a = "1";
                axios.post('/ShowQuextion/Select', { page: a, Title: $('#exampleInputEmail3').val(), GradeName: $('#GradeName').val(), BookName: $('#BookName').val(), ChapterName: $('#ChapterName').val() }).then(Response => {
                    this.QuestionS = Response.data.Li;
                })
                $('#Ye').text(a);
            },
            //分页的下一页
            XiaYe() {
                var a = parseInt($('#Ye').text()) + 1;
                if (parseInt($('#Ye').text()) >= parseInt($('#CuntYe').text())) {
                    alert("已经是最后一页了");
                }
                else {
                    axios.post('/ShowQuextion/Select', { page: parseInt($('#Ye').text()) + 1, Title: $('#exampleInputEmail3').val(), GradeName: $('#GradeName').val(), BookName: $('#BookName').val(), ChapterName: $('#ChapterName').val() }).then(Response => {
                        this.QuestionS = Response.data.Li;
                    })
                    $('#Ye').text(a);
                }
            },
            //分页的上一页
            ShangYe() {
                var a = parseInt($('#Ye').text()) - 1;
                if (a <= 0) {
                    alert("已经是第一页了");
                    $('#Ye').text(1);
                }
                else {
                    axios.post('/ShowQuextion/Select', { page: a, Title: $('#exampleInputEmail3').val(), GradeName: $('#GradeName').val(), BookName: $('#BookName').val(), ChapterName: $('#ChapterName').val() }).then(Response => {
                        this.QuestionS = Response.data.Li;
                    })
                    $('#Ye').text(a);
                }
            },
            //最后一页
            MoYe() {
                var a = $('#CuntYe').text();
                axios.post('/ShowQuextion/Select', { page: a, Title: $('#exampleInputEmail3').val(), GradeName: $('#GradeName').val(), BookName: $('#BookName').val(), ChapterName: $('#ChapterName').val() }).then(Response => {
                    this.QuestionS = Response.data.Li;
                })
                $('#Ye').text(a);
            },
            Resule() {
                $('#exampleInputEmail3').val("");
                this.Init();
                var ops = document.getElementById("GradeName").options;//取出下拉列表的项对象集合
                for (var i = 0; i < ops.length; i++) {//遍历集合
                    if ("全部" == ops[i].text) {//如果该项的value等于文本框填写的值
                        document.getElementById("GradeName").options[i].selected = "selected";//设定下拉列表相应的下拉项的选中
                        break;
                    }
                }
                this.Chapter = [{ ChapterName: "全部" }];
                this.BookList = [{ BookName: "全部" }];
            }
        }
    });
    
